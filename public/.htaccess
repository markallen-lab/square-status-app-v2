<IfModule mod_rewrite.c>
  RewriteEngine On RewriteBase / # If the request is not a file or directory,
  rewrite to index.html RewriteCond %{REQUEST_FILENAME} !-f RewriteCond
  %{REQUEST_FILENAME} !-d RewriteRule ^ index.html [L]
</IfModule>

<IfModule mod_headers.c> Header unset X-Powered-By </IfModule>
